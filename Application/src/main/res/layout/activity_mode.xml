<?xml version="1.0" encoding="utf-8"?>

<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:theme="@style/Theme.AppCompat"
        tools:context=".ModeActivity">

        <TextView
            android:id="@+id/liv_txt"
            android:layout_width="66dp"
            android:layout_height="33dp"
            android:layout_marginStart="48dp"
            android:layout_marginLeft="48dp"
            android:layout_marginTop="130dp"
            android:text="거   실"
            android:textColor="#817F7F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/liv_light"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginTop="10dp"
            android:text="조   명"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/liv_txt" />

        <Switch
            android:id="@+id/liv_light_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/liv_light" />

        <Switch
            android:id="@+id/kit_light_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/kit_light" />

        <Switch
            android:id="@+id/liv_win_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toEndOf="@+id/liv_light_switch"
            app:layout_constraintTop_toBottomOf="@+id/liv_win" />

        <Switch
            android:id="@+id/liv_con_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toEndOf="@+id/liv_win_switch"
            app:layout_constraintTop_toBottomOf="@+id/liv_con" />

        <TextView
            android:id="@+id/liv_win"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="53dp"
            android:layout_marginLeft="53dp"
            android:layout_marginTop="10dp"
            android:text="창   문"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toEndOf="@+id/liv_light"
            app:layout_constraintTop_toBottomOf="@+id/liv_txt" />

        <TextView
            android:id="@+id/liv_con"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="10dp"
            android:text="콘센트"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toEndOf="@+id/liv_win"
            app:layout_constraintTop_toBottomOf="@+id/liv_txt" />

        <Switch
            android:id="@+id/kit_valve_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toEndOf="@+id/kit_light_switch"
            app:layout_constraintTop_toBottomOf="@+id/kit_valve" />

        <Switch
            android:id="@+id/kit_con_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toEndOf="@+id/kit_valve_switch"
            app:layout_constraintTop_toBottomOf="@+id/kit_con" />

        <Switch
            android:id="@+id/room_ligth_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/room_ligth" />

        <Switch
            android:id="@+id/room_win_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toEndOf="@+id/room_ligth_switch"
            app:layout_constraintTop_toBottomOf="@+id/room_win" />

        <Switch
            android:id="@+id/room_con_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toEndOf="@+id/room_win_switch"
            app:layout_constraintTop_toBottomOf="@+id/room_con" />

        <Switch
            android:id="@+id/bath_con_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toEndOf="@+id/bath_light_switch"
            app:layout_constraintTop_toBottomOf="@+id/bath_con" />

        <Switch
            android:id="@+id/bath_light_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginTop="10dp"
            android:switchMinWidth="60dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bath_light" />

        <TextView
            android:id="@+id/kit_light"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginTop="10dp"
            android:text="조   명"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/kit_txt" />

        <TextView
            android:id="@+id/kit_valve"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="53dp"
            android:layout_marginLeft="53dp"
            android:layout_marginTop="10dp"
            android:text="가스밸브"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toEndOf="@+id/kit_light"
            app:layout_constraintTop_toBottomOf="@+id/kit_txt" />

        <TextView
            android:id="@+id/kit_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginLeft="48dp"
            android:layout_marginTop="20dp"
            android:text="주   방"
            android:textColor="#817F7F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/liv_light_switch" />

        <TextView
            android:id="@+id/kit_con"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="10dp"
            android:text="콘센트"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toEndOf="@+id/kit_valve"
            app:layout_constraintTop_toBottomOf="@+id/kit_txt" />

        <TextView
            android:id="@+id/room_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginLeft="48dp"
            android:layout_marginTop="20dp"
            android:text="침   실"
            android:textColor="#817F7F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/kit_light_switch" />

        <TextView
            android:id="@+id/room_ligth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginTop="10dp"
            android:text="조   명"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/room_txt" />

        <TextView
            android:id="@+id/room_con"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="10dp"
            android:text="콘센트"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toEndOf="@+id/room_win"
            app:layout_constraintTop_toBottomOf="@+id/room_txt" />

        <TextView
            android:id="@+id/room_win"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="53dp"
            android:layout_marginLeft="53dp"
            android:layout_marginTop="10dp"
            android:text="창   문"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toEndOf="@+id/room_ligth"
            app:layout_constraintTop_toBottomOf="@+id/room_txt" />

        <TextView
            android:id="@+id/bath_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginLeft="48dp"
            android:layout_marginTop="20dp"
            android:text="화장실"
            android:textColor="#817F7F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/room_ligth_switch" />

        <TextView
            android:id="@+id/bath_light"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginTop="10dp"
            android:text="조   명"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bath_txt" />

        <TextView
            android:id="@+id/bath_con"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="53dp"
            android:layout_marginLeft="53dp"
            android:layout_marginTop="10dp"
            android:text="콘센트"
            android:textColor="#817F7F"
            android:textSize="21sp"
            app:layout_constraintStart_toEndOf="@+id/bath_light"
            app:layout_constraintTop_toBottomOf="@+id/bath_txt" />

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            android:theme="@style/ThemeOverlay.AppCompat.Dark"
            tools:ignore="MissingConstraints">

        </android.support.v7.widget.Toolbar>

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="57dp"
            android:layout_height="54dp"
            android:layout_marginStart="31dp"
            android:layout_marginLeft="31dp"
            android:layout_marginEnd="323dp"
            android:layout_marginRight="323dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar"
            app:srcCompat="@drawable/suitcase" />

    </android.support.constraint.ConstraintLayout>

    <android.support.design.widget.NavigationView
        android:id="@+id/navigation_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:headerLayout="@layout/nav_header"
        app:menu="@menu/drawer"></android.support.design.widget.NavigationView>

</android.support.v4.widget.DrawerLayout>